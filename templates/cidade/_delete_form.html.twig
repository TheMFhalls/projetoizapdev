<form method="post" action="{{ path('cidade_delete', {'id': cidade.id}) }}" onsubmit="return confirm('Tem certeza que dejesa Deletar esta cidade?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ cidade.id) }}">
    <button class="btn btn-danger mt-10">Deletar Cidade</button>
</form>